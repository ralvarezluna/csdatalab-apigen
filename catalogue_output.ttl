@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://scistarter.org/catalogs/> a dcat:Catalog ;
    dct:conformsTo <https://www.iana.org/assignments/media-types/text/turtle>,
        <https://www.w3.org/TR/vocab-dcat-2/> ;
    dct:description "DCAT catalogue of SciStarter to make more discoverable CS projects"@en ;
    dct:publisher <https://scistarter.org> ;
    dct:title "SciStarter FAIR catalogue"@en ;
    dcat:dataset <https://scistarter.org/fresh-eyes-on-ice> ;
    dcat:service <street_story> ;
    foaf:homepage <https://scistarter.org> .

<street_story> a dcat:DataService ;
    dct:description "Obtaining the demodata"@en ;
    dct:title "demodata"@en ;
    dcat:endpointDescription <openapi_test.yaml> ;
    dcat:endpointURL <http://localhost:8080/> ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/application/json>,
        <https://www.iana.org/assignments/media-types/text/html> .

<https://scistarter.org> a foaf:Agent ;
    foaf:name "https://scistarter.org/catalogs/"@en .

<https://scistarter.org/fresh-eyes-on-ice> a dct:Location,
        dcat:Dataset ;
    dct:conformsTo <https://www.w3.org/TR/vocab-dcat-2/> ;
    dct:description """The Fresh Eyes on Ice project is building a network of ice observers to improve public safety on river ice, to support flood forecasting, and to better understand the impacts of climate change. 

You can contribute to this project by sharing your photo observations of river ice conditions using the GLOBE Observer App. Photos of freeze-up, open water leads, ice cover, ice melt, and break-up are all encouraged. 

 GOAL Document river ice conditions all around the world TASKS Take photographs of river ice conditions using the GLOBE Observer App"""@en ;
    dct:issued "2021-05-19"^^xsd:date ;
    dct:language <en> ;
    dct:modified "2021-11-01"^^xsd:date ;
    dct:publisher <https://fresheyesonice.org> ;
    dct:spatial <https://scistarter.org/fresh-eyes-on-ice> ;
    dct:title "Fresh Eyes on Ice"@en ;
    dct:type <https://dbpedia.org/ontology/Project> ;
    dcat:bbox "{'geographic_scope': ''}"^^<http://www.opengis.net/ont/geosparql#asWKT> ;
    dcat:centroid "{'point': {'point': None}}"^^<http://www.opengis.net/ont/geosparql#asWKT> ;
    dcat:contactPoint [ a ns1:Organization ;
            ns1:hasOrganizationName "{'en': None}"@en ;
            ns1:hasURL <https://fresheyesonice.org> ] ;
    dcat:distribution <https://fresheyesonice.org> ;
    dcat:keyword "ice, river, water, GLOBE"@en ;
    dcat:landingPage <https://fresheyesonice.org> .

<https://fresheyesonice.org> a dcat:Distribution,
        foaf:Agent ;
    dcat:accessURL <https://fresheyesonice.org> ;
    foaf:name "{'en': None}"@en .


